{
	"$schema": "https://unpkg.com/release-it@18/schema/release-it.json",
	"git": {
		"commitMessage": "chore: release v${version}",
		"requireBranch": "main",
		"requireCommits": true,
		"tag": true,
		"tagName": "v${version}",
		"push": true
	},
	"github": {
		"release": true,
		"releaseName": "v${version}",
		"autoGenerate": true
	},
	"npm": {
		"publish": false
	}
}
