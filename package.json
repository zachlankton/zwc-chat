{
  "name": "zwc-chat",
  "version": "0.1.0",
  "description": "AI-powered chat application with real-time streaming and multi-model support",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/zachlankton/z3chat.git"
  },
  "keywords": [
    "chat",
    "ai",
    "openai",
    "anthropic",
    "websocket",
    "real-time",
    "bun",
    "react",
    "typescript"
  ],
  "author": "Zachary Lankton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zachlankton/zwc-chat/issues"
  },
  "homepage": "https://github.com/zachlankton/zwc-chat#readme",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:api": "cd apps/api && bun run dev",
    "dev:web": "cd apps/web && bun run dev",
    "build": "bun run build:api && bun run build:web",
    "build:api": "cd apps/api && bun run build",
    "build:web": "cd apps/web && bun run build"
  },
  "devDependencies": {
    "@types/bun": "1.2.15",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "eslint-plugin-react-hooks": "^5.2.0"
  }
}
